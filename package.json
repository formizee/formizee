{
  "name": "formizee",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "prepare": "husky",
    "test": "turbo test",
    "lint": "turbo lint",
    "check": "turbo check",
    "test:ui": "vitest --ui",
    "format": "pnpm biome format --write .",
    "test:coverage": "vitest run --coverage",
    "build": "turbo build --cache-dir=.turbo",
    "local": "pnpm --dir=tools/local local"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/config-conventional": "19.2.2",
    "@formizee/tsconfig": "workspace:*",
    "@vitest/coverage-v8": "2.0.4",
    "@vitest/ui": "2.0.4",
    "commitlint": "19.3.0",
    "husky": "9.0.11",
    "turbo": "2.0.6",
    "typescript": "5.4.5",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.4"
  },
  "packageManager": "pnpm@9.0.5",
  "engines": {
    "node": ">=18"
  }
}
